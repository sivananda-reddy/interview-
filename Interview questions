1).Char *p = “135.145.155.165”;
Write a program to find sum is ?(hint: 135+145+155+165 =600) 


int main() {
    // Write C code here
    char *ip = "135.145.155.165"; //135+145+155+165=600
    printf("Hello world");
    int sum = 0;
    int index = 0;
    char *t = NULL;
    for(int i=0; i<= strlen(ip) ; i++) {
        if((ip[i] == '.') || (ip[i] == '\0')) {
            
            t = (char *)malloc(4);
            memset(t, '\0',4);
            index = i;
            strncpy(t,(&ip[index-3]),4);
            sum = sum + atoi(t);
            free(t);
            t = NULL;
            index = i+1;
        }
        }
        printf("sum:%d\n",sum);
        
   return 0;        
}

Or

int main () {
   char str[80] = "135.145.155.165";
   const char s[2] = ".";
   char *token;
   int sum = 0;
   
   /* get the first token */
   token = strtok(str, s);
   
   /* walk through other tokens */
   while( token != NULL ) {
      printf( "# %s\n", token );
      sum = sum + atoi(token);
      token = strtok(NULL, s);
   }
   printf("sum:%d\n",sum);
   
   return(0);
}


